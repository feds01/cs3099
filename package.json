    "description": "Iamus REST API. CS3099 Project sources.",
    "main": "dist/src/server.js",
    "scripts": {
        "build": "tsc",
        "start": "tsc & node .",
        "start:all": "(yarn start &) && cd frontend && (yarn start &)",
